<query-config>
        <query id="GET_DICT_BY_DICT_STATUS_SINGLE" type="">
        <sql>
            <![CDATA[
                select  d.dict_code,d.dict_text
                from sys_dict d where
                d.dict_type=[dict_type]
                and d.dict_status=1
            ]]>
        </sql>
    </query>

</query-config>